.promo {



    &-title {
        margin-bottom: 43px;
    }

    &-name {
        font-size: $h1;
        color: transparent;
        -webkit-text-stroke: 1px white;
        line-height: 1;
        margin-bottom: 80px;

        @include phones {
            font-size: 50px;
        }
    }
    @include tablets {
        text-align: center;
    }
}